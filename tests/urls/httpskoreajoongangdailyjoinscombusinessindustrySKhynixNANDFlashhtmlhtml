<script>
    $(document).ready(function() {
        $('.tag_photoslide').bxSlider({
            slideSelector: 'div.ab_photo'
        });

        var serviceDate = "20201207153031";
        var week = moment(serviceDate, 'YYYYMMDDHHmmss').format('dddd');
        var monthAndDay = moment(serviceDate, 'YYYYMMDDHHmmss').format('LL');
        

        $(".twentytwenty-container").twentytwenty();
        $('#present_week').html(week);
        $('#present_month_day').html(monthAndDay);

        setFontSize();

        //기사 load시 section Array 생성
        setSectionCount();
        // getSectionArticleList();
    });

    var analyticData = {
        articleId : "3084020",
        title : "SK hynix begins offering samples of 176-layer NAND chip",
        cmss_url : "https://koreajoongangdaily.joins.com/2020/12/07/business/industry/SK-hynix-NAND-Flash/20201207153100497.html",
        siteCd : "JAD",
        hp_thumbnail_img : "https://koreajoongangdaily.joins.com/data/photo/thumbnail/2020/12/07/46b7e64e-0401-4065-8de5-2dd59e377dff.jpg",
        topMenu : "business",
        subMenu : "industry",
        section : "Industry",
        reporters : "[{code=park.eunjee1, jcms_rep_seq=0, name=박은지, jam_rep_seq=2583, blog=, email=park.eunjee@joongang.co.kr}]"
    }

    var reporters = '[{code=park.eunjee1, jcms_rep_seq=0, name=박은지, jam_rep_seq=2583, blog=, email=park.eunjee@joongang.co.kr}]';

    if(reporters != '[]' && reporters != ""){
        reporters = reporters.match(/name\=.*?\,/)[0].replace("name=","").replace(",","")
        analyticData.reporters = reporters;
    }

    var fs = 3; // 폰트사이즈


    function setFontSize(t) {
        if (t == 'tm') {
            fs--;
        } else {
            fs++;
        }
        if (fs <= 1) {
            fs = 1;
        } else if (fs >= 5) {
            fs = 5;
        }
        $('#article_body').removeClass('fs1 fs2 fs3 fs4 fs5');
        $('#article_body').addClass('fs' + fs);
        // JCookie.SetCookie('ListPreviewFontSize', fs, 365);
    }


    //화면 인쇄
    function pagePrint() {

        var title = "SK hynix begins offering samples of 176-layer NAND chip";
        $('#article_data').printThis({
            pageTitle : title,
            importCSS : true,
            loadCSS : [
                "/resources/css/lib/print/print.css"
            ],
            removeInline : false
        });
        //setTimeout("showUpt()", 334);
    }
    //이미지 뷰어
    $('.image > a').click(function(e){

        // 만약 이미지 뷰어가 슬라이드 뷰어로 전환되었던 상태라면 초기화
        var src = $(this).children('img').attr('src');

        var image = "<div class='image_center' style='width:100%; height:100%'><img id='imgViewer' src='"+src+"' style='width:auto;max-width:100%;height:auto;'></div>";
        $(".image_area").append(image);
        // 클릭 된 부모요소 안에 이미지가 여러장일 경우는 슬라이드로 변경해야함
        if($(this).children('img').length > 1){
            e.preventDefault();
            $(".image_area").addClass('slick');
            fn_showSlider($(this));
        }else{
            $("#imgViewer").attr('src',src);
            $("#imgViewer").css('width','auto');
            $("#imgViewer").css('max-width','100%');
            $("#imgViewer").css('height','auto');
            $("#image_viewer").css('display','block');
        }
    })

    function fn_showViewer(src){
        $("#imgViewer").attr('src',src);
        $("#imgViewer").css('width','auto');
        $("#imgViewer").css('max-width','100%');
        $("#imgViewer").css('height','auto');
        $("#image_viewer").css('display','block');
    }
    function fn_hideViewer(){
        $("#image_viewer").css('display','none');

        if($('.image_area').hasClass('slick')){
            $('.slick').slick('unslick');
            $('.image_center').each(function(idx,item){
                $(item).remove();
            });
        }
    }

    function fn_showSlider(parent){
        var imgCount = parent.children('img').length;
        if(imgCount > 1){
            $(".image_area").addClass('slick');
            $(".image_area").empty();
        }

        parent.children('img').each(function(idx,item){
            var src = $(item).attr('src');
            var slide = "<div class='image_center slick-slide' style='width:100%; height:100%'><img id='imgViewer' src='"+src+"' style='width:auto;max-width:100%;height:auto;'></div>";
            $(".slick").append(slide);
        })
        slickInit();
    }

    function slickInit(){
        setTimeout(function(){
            $('.slick').slick({
                dots: false,
                infinite: true,
                speed: 300,
                slidesToShow: 1,
                adaptiveHeight: true,
                fade: true,
                cssEase: 'linear'
            });
        }, 100);


        $("#image_viewer").css('display','block');
    }
    var progressIndicator = '<div id="i-am-progress-indicator"></div>';
    $('body').append(progressIndicator);
    $('#i-am-progress-indicator').css('display','none')

    $(window).on('scroll', function(){
        var viewDocument = $('#doc');
        var viewDocumentHeight = viewDocument.outerHeight();
        var viewDocumentOffset = viewDocument.offset().top;
        var stickyHeaderHeight = $('#header').outerHeight();
        var startProgress = viewDocumentOffset - stickyHeaderHeight;
        var currentPercentage = ($(window).scrollTop() - startProgress ) / viewDocumentHeight * 100;
        $('#i-am-progress-indicator').css({
            display: 'block',
            width: currentPercentage+'%'
        });
    });

    function getSectionArticleList(){
        //count가 높은 section을 size 만큼 보여준다.
        var size = 3;
        var obj = getSectionCount(size);
        var cmssId = "3084020";

        //KEY VALUE LIST. 필요.
        $.ajax({
            url: '/article/sectionArticleList',
            type: 'POST',
            data: {"articleList" : JSON.stringify(obj), "size": size, "cmssId" : cmssId},
            success: function (response) {
                //mustache
                if(response != null){
                    var toHtml = ""
                    for(var i = 0; i<response.length; i++){
                        toHtml += Mustache.to_html(likeArticleTemplate, response[i].RESULT_LIST[0])
                    }

                    $("#likeArticleBox").html(toHtml);
                    $("#listArticleBoxTitle").css('display','block');
                }

            }
        });
    }

    var likeArticleTemplate = '<div class = "col-md-4">';
    likeArticleTemplate += '<a title = "{{{title}}}" href = "{{{cmss_url}}}">';
    likeArticleTemplate += '{{#hp_thumbnail_img}}<div class="thumbnailBox">';
    likeArticleTemplate += '<span class = "img-mask-wrap" style="overflow: hidden"><img class="img-fluid" src="{{{hp_thumbnail_img}}}" style="width:auto; height: 100%" alt="" title=""></span>';
    likeArticleTemplate += '</div>{{/hp_thumbnail_img}}';
    likeArticleTemplate += '</a>';
    likeArticleTemplate += '<a title="{{{title}}}" href="{{{cmss_url}}}"> <span> <p class = "like-article-text">{{{list_title}}}</p></span> </a></div>';

</script><hr class="row mobile-none"/>

